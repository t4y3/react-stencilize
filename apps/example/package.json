{
  "name": "@react-stencilize/example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "check": "npm run typecheck && npm run biome:check",
    "typecheck": "tsc -b --pretty false",
    "biome:check": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:staged": "biome check --write --no-errors-on-unmatched",
    "format": "biome format --write .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.20.0",
    "@tailwindcss/vite": "4.1.17",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-stencilize": "*",
    "tailwindcss-skeleton-screen": "2.0.0"
  },
  "devDependencies": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.0.0",
    "typescript": "5.9.3",
    "vite": "7.2.2"
  }
}
